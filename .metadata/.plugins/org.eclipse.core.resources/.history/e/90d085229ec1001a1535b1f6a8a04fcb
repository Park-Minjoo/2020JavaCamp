import java.awt.Color;

import javax.swing.Timer;

public class Main {
   private static Frame f = null;
   private static boolean finishCheck ;
   private static choiceRock cr;
   public static void main(String[] args) {
      
      f =new Frame();
      f.setVisible(true);
      
      cr = new choiceRock();
      cr.setVisible(true);
      
      
   }
   
   public static void setFinishCheck(boolean check ) {
      finishCheck = check;
      f.setVisible(false);
      f.setVisible(true);
   }
   
   public static choiceRock getChoiceFrame() {
      return cr;
   }
}